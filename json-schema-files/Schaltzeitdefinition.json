  "type": "object",
  "properties": {
    "boTyp": {
      "type": ["string", "null"]
    },
    "versionStruktur": {
      "type": ["string", "null"]
    },
    "timestamp": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "externeReferenzen": {
      "type": ["array", "null"],
