            "STUNDE",
            "VIERTEL_STUNDE",
            "TAG",
            "WOCHE",
            "MONAT",
            "QUARTAL",
            "HALBJAHR",
            "JAHR"
          ]
        },
        "dauer": {
          "type": ["number", "null"]
        },
        "startdatum": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "enddatum": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "startzeitpunkt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "endzeitpunkt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "timestamp": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "guid": {
          "type": ["string", "null"]
        }
