            null,
            "SEKUNDE",
            "MINUTE",
            "STUNDE",
            "VIERTEL_STUNDE",
            "TAG",
            "WOCHE",
            "MONAT",
            "QUARTAL",
            "HALBJAHR",
            "JAHR"
          ]
        },
        "dauer": {
          "type": ["number", "null"]
        },
        "startdatum": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "enddatum": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "startzeitpunkt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "endzeitpunkt": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "timestamp": {
          "type": ["string", "null"],
          "format": "date-time"
        },
        "guid": {
          "type": ["string", "null"]
        }
      },
      "required": [
        "einheit",
        "dauer",
        "startdatum",
        "enddatum",
        "startzeitpunkt",
        "endzeitpunkt"
      ]
    }
  },
  "type": "object",
  "properties": {
    "boTyp": {
      "type": ["string", "null"]
    },
    "versionStruktur": {
      "type": ["string", "null"]
    },
    "timestamp": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "externeReferenzen": {
      "type": ["array", "null"],
      "items": {
        "$ref": "#/definitions/ExterneReferenz"
      }
    },
    "guid": {
      "type": ["string", "null"]
    },
    "lokationsId": {
      "type": ["string", "null"]
    },
    "lokationsTyp": {
      "type": "string",
      "enum": ["MALO", "MELO"]
    },
    "obiskennzahl": {
      "type": ["string", "null"]
    },
    "ZeitraumMesswertanfrage": {
