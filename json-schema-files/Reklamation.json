    "reklamationsgrundBemerkung": {
      "type": ["string", "null"]
    },
    "zeitpunktFuerWertanfrage": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "gueltigkeitszeitraum": {
      "$ref": "#/definitions/Zeitraum"
    },
    "qualitaet": {
      "type": ["string", "null"],
      "enum": [
        null,
        "VOLLSTAENDIG",
        "INFORMATIV",
        "IM_SYSTEM_VORHANDEN",
        "ERWARTET",
        "VORLAEUFIG",
        "UNVOLLSTAENDIG"
      ]
    }
  },
  "required": [
    "boTyp",
    "versionStruktur",
    "externeReferenzen",
    "lokationsId",
    "lokationsTyp",
    "obiskennzahl",
    "ZeitraumMesswertanfrage",
